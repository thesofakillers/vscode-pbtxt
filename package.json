{
    "name": "vscode-pbtxt",
    "displayName": "Protobuf Text Format",
    "description": "Protocol Buffer Text Format syntax highlighting for VS Code",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pbtxt",
                "aliases": [
                    "Protocol Buffer Text Format",
                    "pbtxt",
                    "Textual Protobuf"
                ],
                "extensions": [
                    ".pbtxt",
                    ".prototxt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pbtxt",
                "scopeName": "source.pbtxt",
                "path": "./syntaxes/pbtxt.tmLanguage.json"
            }
        ]
    },
    "publisher": "thesofakillers",
    "repository": {
        "type": "git",
        "url": "https://github.com/thesofakillers/vscode-pbtxt"
    },
    "icon": "logo.png"
}
